{
  "diagnostic.displayByAle": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "gocodeCompletionEnabled": true,
        "diagnosticsEnabled": true,
        "lintTool": "golint"
      }
    }
  },
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true
}
